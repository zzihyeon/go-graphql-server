# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Stock {
  id: ID!
  name: String!
  volume: String!
  reason: [String!]!
}

type Query {
  stocks: [Stock!]!
}

input NewStock {
  id: ID!
  name: String!
  volume: String!
  reason: [String!]!
}

type Mutation {
  createStock(input: NewStock!): Stock!
}